{
	"name": "jobs-admin",
	"private": true,
	"scripts": {
		"start": "umi dev",
		"build": "umi build",
		"start-test": "cross-env UMI_ENV=test umi dev",
		"build-test": "cross-env UMI_ENV=test umi build",
		"start-release": "cross-env UMI_ENV=release umi dev",
		"build-release": "cross-env UMI_ENV=release umi build",
		"postinstall": "umi generate tmp",
		"prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
		"test": "jest",
		"test:coverage": "jest --coverage"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js?(x),ts(x),less,md,json}": [
			"prettier --write"
    ],
    "*.{js?(x),ts(x)}": [
			"eslint --fix"
		],
		"*.ts?(x)": [
			"prettier --parser=typescript --write"
		]
	},
	"dependencies": {
		"@ant-design/pro-layout": "^5.0.12",
		"@umijs/plugin-qiankun": "2.5.1",
		"@umijs/preset-react": "1.x",
		"@umijs/test": "^3.2.10",
		"ahooks": "^2.2.0",
		"antd-img-crop": "^3.10.0",
		"braft-editor": "^2.3.9",
		"js-base64": "^3.5.2",
		"lint-staged": "^10.0.7",
		"mathjs": "^7.1.0",
		"prettier": "^1.19.1",
		"qs": "^6.6.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-viewerjs": "^0.1.9",
		"umi": "^3.2.10",
		"uuid": "^8.3.0",
		"wangeditor": "^4.4.2",
		"yorkie": "^2.0.0"
	},
	"devDependencies": {
		"@types/jest": "^26.0.20",
		"@typescript-eslint/eslint-plugin": "^3.6.1",
		"@typescript-eslint/parser": "^3.6.1",
		"cross-env": "^7.0.2",
		"eslint": "^7.5.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"jest": "^26.6.3",
		"ts-node": "^9.1.1",
		"typescript": "^3.9.7"
	}
}
