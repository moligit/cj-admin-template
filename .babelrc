{
  "presets": [
    [
        "@babel/preset-env",
        {
            "targets": {
                "node": "current"
            }
        }
    ]
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    [
      "babel-plugin-transform-require-ignore",
      {
        "extensions": [".less", ".sass"]
      }
    ]
  ]
}
